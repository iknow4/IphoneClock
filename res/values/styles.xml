    </style>
    <style name="IPhone_Left_Arrow_Shape_Button_Style" parent="@style/IPhone_Button_Style">
        <item name="android:background">@drawable/iphone_button_arraw_bg</item>
    </style>
    <style name="IPhone_Button_Right_LightBlue_Style" parent="@style/IPhone_Button_Style">
        <item name="android:background">@drawable/iphone_button_right_bg_1</item>
    </style>
    <style name="Iphone_Label_Check_View_Style">
        <item name="android:clickable">true</item>
